{
    "name": "neputertech/getpay-gateway",
    "description": "Laravel integration for GetPay Nepal payment gateway integration",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "NeputerTech\\GetpayGateway\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nitish Raj Uprety",
            "email": "nitishuprety@neputer.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/view": "^9.0|^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NeputerTech\\GetpayGateway\\GetpayServiceProvider"
            ],
            "aliases": {
                "Getpay": "NeputerTech\\GetpayGateway\\Facades\\Getpay"
            }
        }
    }
}
